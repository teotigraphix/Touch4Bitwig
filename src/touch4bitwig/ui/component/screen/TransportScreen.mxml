<?xml version="1.0"?>
<f:Screen xmlns:f="library://ns.feathersui.com/mxml"
          xmlns:fx="http://ns.adobe.com/mxml/2009"
          xmlns:main="touch4bitwig.ui.component.main.*">

    <f:layout>
        <f:VerticalLayout/>
    </f:layout>

    <main:TransportBar>
        <main:layoutData>
            <f:VerticalLayoutData percentWidth="100" percentHeight="100"/>
        </main:layoutData>
    </main:TransportBar>

    <f:LayoutGroup>
        <f:layoutData>
            <f:VerticalLayoutData percentWidth="100" percentHeight="100"/>
        </f:layoutData>
    </f:LayoutGroup>

    <f:LayoutGroup>
        <f:layoutData>
            <f:VerticalLayoutData percentWidth="100" percentHeight="100"/>
        </f:layoutData>
    </f:LayoutGroup>

</f:Screen>