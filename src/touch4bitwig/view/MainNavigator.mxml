<ui:BootstrapNavigator xmlns:fx="http://ns.adobe.com/mxml/2009"
                       xmlns:f="library://ns.feathersui.com/mxml"
                       xmlns:ui="com.teotigraphix.app.ui.*">

    <f:StackScreenNavigatorItem id="ConfigurationScreen"
                                screen="{configurationScreen}">
    </f:StackScreenNavigatorItem>

    <f:StackScreenNavigatorItem id="MixerScreen"
                                screen="{mixerScreen}">
    </f:StackScreenNavigatorItem>

    <f:StackScreenNavigatorItem id="TransportScreen"
                                screen="{transportScreen}">
    </f:StackScreenNavigatorItem>

    <f:StackScreenNavigatorItem id="PanelsScreen"
                                screen="{panelsScreen}">
    </f:StackScreenNavigatorItem>

    <fx:Script><![CDATA[
        import touch4bitwig.view.screen.ConfigurationScreen;
        import touch4bitwig.view.screen.MixerScreen;
        import touch4bitwig.view.screen.PanelsScreen;
        import touch4bitwig.view.screen.TransportScreen;

        private const a:ApplicationScreens;

        public static const configurationScreen:Class = ConfigurationScreen;
        public static const mixerScreen:Class = MixerScreen;
        public static const transportScreen:Class = TransportScreen;
        public static const panelsScreen:Class = PanelsScreen;
        ]]></fx:Script>

</ui:BootstrapNavigator>